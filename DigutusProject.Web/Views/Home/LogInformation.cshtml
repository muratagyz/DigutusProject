@model List<DigutusProject.Core.Models.Log>
@if (Model != null)
{
    <div class="container">
        <b>Toplam Sayı : @Model.Count </b><br />
        <i>(Sadece son 1 günü gösterilir.)</i>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Email</th>
                    <th scope="col">Son Giriş Tarihi</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <td>@item.Email</td>
                        <td>@item.CreateDate</td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
}

